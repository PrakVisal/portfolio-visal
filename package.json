{
  "name": "portfolio-nextjs",
  "version": "1.0.0",
  "private": true,
  "description": "A modern, responsive portfolio website built with Next.js, TypeScript, and Tailwind CSS",
  "keywords": [
    "portfolio",
    "nextjs",
    "typescript",
    "tailwindcss",
    "react",
    "web-development"
  ],
  "homepage": "https://yourportfolio.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/portfolio-nextjs.git"
  },
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "url": "https://yourwebsite.com"
  },
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next out dist",
    "db:admin": "node scripts/create-admin.js",
    "db:init": "node scripts/init-db.js",
    "db:migrate": "npm run db:init && npm run db:admin",
    "db:status": "node scripts/check-db.js",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "echo Installation complete!",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky install || true",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "start": "next start",
    "test:format": "npm run format:check",
    "test:lint": "npm run lint",
    "test:type": "npm run type-check",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
